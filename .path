# ZSH setup
export DOTFILES=$HOME/.dotfiles

# RUBY
export PATH=$PATH:$HOME/.gem/ruby/2.6.0/bin

# Golang
export GOROOT=/usr/local/opt/go/libexec
export GOPATH=$HOME/softwares/go
export PATH=$PATH:$GOROOT/bin
export PATH=$PATH:$GOPATH/bin

# pyenv files
export PYENV_ROOT="$HOME/.pyenv"
export PATH="$PYENV_ROOT/bin:$PATH"

export Boost_INCLUDE_DIRS=/usr/local/Cellar/boost/1.73.0/include

# Local files
export PATH=$PATH:/usr/local/bin
export PATH=$PATH:/usr/local/sbin
export PATH=$PATH:/usr/local/mysql/bin

export PAHT=$PATH:/usr/local/Cellar/neovim/0.4.4/bin

export PATH=$PATH:$HOME/softwares/anaconda3/bin

#Dart
export PATH=$PATH:$HOME/softwares/flutter/bin
export PATH=$PATH:$HOME/softwares/flutter/bin/cache/dart-sdk/bin/
export PATH=$PATH:$HOME/.pub-cache/bin

# Java
export JAVA_HOME=`/usr/libexec/java_home -v 17`
export PATH="/usr/local/opt/openjdk/bin:$PATH"
export CPPFLAGS="-I/usr/local/opt/openjdk/include"

# Android
export ANDROID_SDK=$HOME/softwares/androidsdk
export PATH=$PATH:$ANDROID_SDK/platform-tools
export PATH=$PATH:$ANDROID_SDK/emulator
